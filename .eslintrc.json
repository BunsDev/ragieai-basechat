{
  "extends": "next/core-web-vitals",
  "plugins": ["import", "react"],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ],
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ],
    "react/no-unknown-property": ["error", { "ignore": ["css"] }]
  }
}
